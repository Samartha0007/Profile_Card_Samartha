{
  "version": 2,
  "builds": [
    { "src": "profile.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/([a-zA-Z0-9_-]+)",
      "dest": "/profile.js",
      "headers": {
        "Cache-Control": "no-store, max-age=0",
        "Content-Type": "text/html"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}